    "pg": "^7.4.1",
    "pg-camelcase": "^0.0.3"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.2",
    "prettier-eslint-cli": "^4.7.1"
